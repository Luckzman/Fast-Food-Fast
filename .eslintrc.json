{
    "extends": "airbnb",
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "no-restricted-syntax": "off",
        "consistent-return": "off",
        "no-shadow": "off",
        "camelcase": "off",
        "array-callback-return": "off",
        "no-undef":"off",
        "no-alert":"off",
        "no-unused-expressions":"off",
        "guard-for-in":"off",
        "no-loop-func":"off",
        "no-prototype-builtins":"off"
    },
    "env": {
        "mocha": true
    }
}